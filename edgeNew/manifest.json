{
  "name": "ResponseDecryptor",
  "description": "A Basic DevTools Extension",
  "manifest_version": 2,
  "version": "1.0",
  "devtools_page": "devtools.html",
  "permissions": [
    "storage",
    "system.memory",
    "webRequest",
    "webRequestBlocking",
    "*://www.zaixiankaoshi.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}