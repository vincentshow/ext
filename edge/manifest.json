{
    "name": "DevTools test edge",
    "description": "A Basic DevTools Extension",
    "manifest_version": 3,
    "version": "1.0",
    "devtools_page": "devtools.html",
    "permissions": [
        "system.memory"
      ],
      "content_scripts": [{
        "matches": [
          "http://*/*",
          "https://*/*"
        ],
        "run_at": "document_idle",
        "js": [
          "content_script.js"
        ]
      }],
      "background": {
          "service_worker": "background.js"
      }
}